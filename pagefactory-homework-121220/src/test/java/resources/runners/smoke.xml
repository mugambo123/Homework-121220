<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1" >
    <groups>
        <run>
            <include name="smoke"></include>
        </run>
    </groups>
    <test name="Last Minutes Deals Sort by Low To High Prices" >
        <classes>
            <class name="uk.co.amazon.testsuite.DealsSortByLowToHigh"></class>
            <class name="uk.co.amazon.testsuite.GetListOfHighestDiscountedProducts"></class>
            <class name="uk.co.amazon.testsuite.NewCustomerCreateAccount"></class>
        </classes>
    </test>
</suite>
